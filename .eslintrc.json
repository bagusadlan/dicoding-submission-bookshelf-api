{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "arrow-parens": ["error", "as-needed"],
        "semi": "off",
        "linebreak-style": 0,
        "indent": [2, "tab"],
        "no-tabs": 0,
				"eol-last": 0,
				"no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
    }
}
